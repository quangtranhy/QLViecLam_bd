<script>

    function getListXa() {
        var mahuyen = document.getElementById("huyen_create").value;
        var maxa = document.getElementById("xa_create");

        maxa.innerHTML = "";
        fetch(`/DanhsachDN/GetXa?mahuyen=${mahuyen}`)
            .then(response => response.json())
            .then(data => {
                data.forEach(function (item) {
                    var option = document.createElement("option");
                    option.value = item.maquocgia;
                    option.text = item.name;
                    maxa.add(option);
                });
            })
            .catch(error => console.error('Error:', error));

    }

    function SetPrint(id) {
        document.getElementById("id_print1").value = id;
    }

</script>