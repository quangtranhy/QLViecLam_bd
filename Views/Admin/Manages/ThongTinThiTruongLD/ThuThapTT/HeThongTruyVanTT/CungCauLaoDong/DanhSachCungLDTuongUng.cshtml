@{
    var record_id = 1;
    Layout = null;
}
@model IEnumerable<QLViecLam.Models.Admin.Manages.NhanKhau>
<table class="table table-striped table-bordered table-hover" id="sample_3">
    <thead>
        <tr style="text-align:center">
            <th>Stt</th>
            <th>Họ tên</th>
            <th>CMND</th>
            <th>Ngày sinh</th>
            <th>Thường trú</th>            
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            @foreach (var item in Model)
            {
                <tr>
                    <td>@(record_id++)</td>
                    <td>@item.HoVaTen</td>
                    <td>@item.SoCCCD</td>
                    <td>@item.NgayThangNamSinh</td>
                    <td>@item.ThuongTru</td>   
                </tr>
            }
        }
    </tbody>
</table>