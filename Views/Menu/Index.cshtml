}
@section custom_style {
    <link rel="stylesheet" type="text/css" href="~/Admin/assets/datatables/bootstrap/dataTables.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Admin/assets/datatables/bootstrap/dataTables.css" />
    <link rel="stylesheet" type="text/css" href="~/NewFolder/test.css" />

}


@if (phanloaitk == "1")
{
    @await Html.PartialAsync("~/Views/Menu/Manages/KeHoachThuThap.cshtml")
    @await Html.PartialAsync("~/Views/Menu/Manages/CungLaoDong.cshtml")
    @if (CapDo == "T")
    {
        @await Html.PartialAsync("~/Views/Menu/Manages/DoanhNghiep.cshtml")
    }
    @await Html.PartialAsync("~/Views/Menu/Manages/BaoCaoThongKe.cshtml")
    @await Html.PartialAsync("~/Views/Menu/Manages/BanTin.cshtml")
    @await Html.PartialAsync("~/Views/Menu/Manages/HeThong.cshtml")


}
@if (phanloaitk == "2")
{
    @await Html.PartialAsync("~/Views/Menu/Employer/Employer.cshtml")

}
