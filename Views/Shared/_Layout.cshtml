            $(document).ready(function () {
                var menu1 = document.getElementById("MenuLv1").value;
                var menu2 = document.getElementById("MenuLv2").value;
                var menu3 = document.getElementById("MenuLv3").value;
                if (menu3 != '') {
                    document.getElementById(menu1).className = "menu-item menu-item-open menu-item-here";
                    document.getElementById(menu2).className = "menu-item menu-item-submenu menu-item-open menu-item-here";
                    document.getElementById(menu3).className = "menu-item menu-item-active";
                } else if (menu2 != '') {
                    document.getElementById(menu1).className = "menu-item menu-item-open";
                    document.getElementById(menu2).className = "menu-item menu-item-active";
                } else if (menu1 != '') {
                    document.getElementById(menu1).className = "menu-item menu-item-active";
                }
            });

            function MoneyMask() {
                $('.money').mask('000,000,000,000,000', {
                    reverse: true
                });
            }

            function SetValue(value, id_set) {
                document.getElementById(id_set).value = value;
            }

            function display_ct6() {
                var x = new Date()
                var month = x.getMonth() + 1;
                var date = x.getDate();
                var year = x.getFullYear();
                var hour = x.getHours();
                var minute = x.getMinutes();
                var second = x.getSeconds();

                if (date < 10) { date = '0' + date }
                if (month < 10) { month = '0' + month }
                if (hour < 10) { hour = '0' + hour }
                if (minute < 10) { minute = '0' + minute }
                if (second < 10) { second = '0' + second }

                var x1 = date + "/" + month + "/" + year;
                x1 = x1 + " - " + hour + ":" + minute + ":" + second;
                document.getElementById('ct6').innerHTML = x1;
                display_c6();
            }
            function display_c6() {
                var refresh = 1000; // Refresh rate in milli seconds
                mytime = setTimeout('display_ct6()', refresh)
            }
            display_c6()
        </script>

